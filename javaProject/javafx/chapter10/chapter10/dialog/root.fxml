<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>


<HBox fx:id="hBox" spacing="10.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="chapter10.dialog.RootController">
   <children>
      <Button mnemonicParsing="false" onAction="#handleOpenFileChooser" text="Open FileChooser" />
      <Button mnemonicParsing="false" onAction="#handleSaveFileChooser" text="Save FileChooser" />
      <Button mnemonicParsing="false" onAction="#handleDirectoryFileChooser" text="Directory FileChooser" />
      <Button mnemonicParsing="false" onAction="#handlePopup" text="Popup" />
      <Button mnemonicParsing="false" onAction="#handleCustom" text="Custom" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>

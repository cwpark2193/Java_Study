package chapter9.rectangle;

public class RectangleTest {

	public static void main(String[] args) {
		Rectangle r1 = new Rectangle(10, 20);
		Rectangle r2 = new Rectangle(20, 10);
		Circle c1 = new Circle(100);
		System.out.println(r1);
		System.out.println(r2);
		System.out.println(c1);
		int returnValue = r1.compareTo(r2);
		switchFunction(returnValue, r1, r2);;
		returnValue = r1.compareTo(c1)
		switchFunction(returnValue, r1, c1);
	}
	public static void switchFunction(int returnValue,String str1,String str2) {
		switch(returnValue) {
		case 1 : System.out.println("객체1이 더 작습니다."); break;
		case -1 : System.out.println("객체1이 더 큽니다."); break;
		case 0 : System.out.println("두 객체가 같습니다"); break;
		}
		return;
	}
}

package Q17;

public class Circle implements Movable {

	@Override
	public void move(int dx, int dy) {
		dx=dx*(-1);
		dy=dy*(-1);
	}

	public Circle() {
		super();
		// TODO Auto-generated constructor stub
	}
	public void getPoint(int dx,int dy) {
		System.out.println("dx= "+dx+" dy = "+dy);
		return ;
	}

}

package 객체지향14;

public class Q2{
	 public static void main(String[] args) {
         sub();
   }
   public static void sub() {
          int[] array = new int[10];
          int i =0;
          try {
        	  i = array[10];
          }catch(ArrayIndexOutOfBoundsException a) {
        	  System.out.println("오류를 잡아보아요~");
          }finally {
			System.out.println("오류 캐치 완료!");
		}
   }

}
package Q17;

public class Triangle implements Movable {

	@Override
	public void move(int dx, int dy) {
		dx=dx*(1);
		dy=dy*(-1);
		System.out.println("삼각형의 기준점이 "+dx+","+dy+"로 변경되었습니다.");
	}

	public Triangle() {
		super();
		// TODO Auto-generated constructor stub
	}

	@Override
	public String toString() {
		return "Triangle [toString()=" + super.toString() + "]";
	}	
	

}

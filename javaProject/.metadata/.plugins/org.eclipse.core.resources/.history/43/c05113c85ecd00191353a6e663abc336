package Q22.game;

public class DieTest {

	public static void main(String[] args) {
		Die humanValue = new Die();
		Die comValue = new Die();
		humanValue.roll();
		comValue.roll();
		int returnValue = humanValue.compareTo(comValue);
	}
	public void compare(int returnValue) {
		switch(returnValue) {
		case 1 : System.out.println("사용자가 이겼습니다.");
		case -1 : System.out.println("컴퓨터가 이겼습니다.");
		case 0 : System.out.println("둘이 비겼습니다.");
		}
	}
}

package chapter15.test;

import chapter15.myclass.MyArrayAlg;
import chapter15.myclass.MyClass;

public class MyClassTest {

	public static void main(String[] args) {
		MyClass[] mcArray = {new MyClass<MyClass>(10,"고길동"), new MyClass<MyClass>(20,"구길동"),new MyClass<MyClass>(30,"홍길동")};
		
		MyClass largeObject = MyMethod.getMax(mcArray);
		MyClass largeString = MyMethod.getMax(mcArray);
		System.out.println(largeString+"이 가장 큰 객체입니다.");
		System.out.println(largeObject+"이 가장 큰 객체입니다.");
		
		
		
		
	}

}
